package com.variables;

 class Variables_01 {
	int a = 12;
	int b = 14;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Variables_01.m1();
		Variables_01.m2();

		
	}

static void m1(){
	Variables_01 v = new Variables_01();
		System.out.println("from m1 "+v.a);
		System.out.println("from m1 "+v.b);
		
	}
	
	
	static void m2(){

		Variables_01 v1 = new Variables_01();
		System.out.println("from m2 "+v1.a);
		System.out.println("from m2 "+v1.a);

	}
}